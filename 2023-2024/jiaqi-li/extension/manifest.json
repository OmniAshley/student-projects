{
    "manifest_version": 3,
    "name": "Content Reader",
    "version": "1.0",
    "description": "Reads main content from all opened pages",
    "permissions": [
        "activeTab",
        "storage",
        "webNavigation"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}